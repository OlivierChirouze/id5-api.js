{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "browsers": [
            "chrome >= 61",
            "safari >= 11",
            "edge >= 14",
            "firefox >= 57",
            "ios >= 11",
            "node >= 18"
          ]
        }
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-object-assign"
  ]
}
